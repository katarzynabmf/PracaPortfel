@{
}
@model IEnumerable<Portfel.Data.Data.Transakcja>
<h1>Wszytskie transakcje</h1>
<h2>Witaj </h2>
@foreach (var item in Model)
{
    <tr>Transakcja: @item.Id
        <tr>@item.Konto</tr><br>
        <tr>@item.KontoId</tr><br>
        <tr>@item.RodzajOplaty</tr><br>
        <tr>@item.Kwota</tr><br>
        <tr>@item.Komentarz</tr><br>
        <tr>@item.RodzajTransakcji</tr><br>
    </tr>
}
